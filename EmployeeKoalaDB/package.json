{
  "name": "employeekoaladb",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node app",
    "clean": "rm -rf ./dist",
    "setup": "npm run clean && mkdir -p ./dist",

    "build": "npm run build:jade",
    "build:jade": "jade views/index.jade -o dist",
    "aws": "aws s3 sync dist/ s3://diego-servless-harness-node-app --acl public-read",
    "deploy": "npm run setup && npm run build && npm run aws"
  },
  "dependencies": {
    "connect": "^3.7.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "errorhandler": "^1.5.1",
    "express": "3.3.5",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "method-override": "^3.0.0",
    "mongodb": ">= 0.9.6-7",
    "morgan": "~1.9.1",
    "serve-favicon": "^2.5.0",
    "stylus": "0.54.5"
  }
}
